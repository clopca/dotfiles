{
  "[jsonc]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[python]": {
    "editor.formatOnType": true
  },
  "[typescript]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[yaml]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "amazonQ.suppressPrompts": {
    "amazonQChatDisclaimer": true,
    "amazonQChatPairProgramming": true
  },
  "amazonQ.telemetry": false,
  "cSpell.language": "en,es-ES",
  "cSpell.userWords": [
    "AEAT",
    "affiliateds",
    "aprobacion",
    "cedula",
    "clopca",
    "cloudflare",
    "codigo",
    "colombia",
    "Configuracion",
    "creditea",
    "crediteame",
    "cristobal",
    "dana",
    "declaracion",
    "direccion",
    "documentacion",
    "Documentacion",
    "Electronico",
    "Ertes",
    "ERTES",
    "fontkit",
    "gearshape",
    "guia",
    "hasher",
    "heroicons",
    "informacion",
    "investtup",
    "Investtup",
    "nativewind",
    "neondatabase",
    "neverthrow",
    "nextjs",
    "Objectv",
    "Oidc",
    "openauth",
    "openauthjs",
    "postgres",
    "POSTHOG",
    "presigner",
    "Pressable",
    "prestamo",
    "pulumi",
    "pulumiverse",
    "razon",
    "sonner",
    "Sonner",
    "unenv",
    "unsplash",
    "valibot",
    "verificator"
  ],
  "codeviz.analyticsEnabled": true,
  "console-ninja.featureSet": "Community",
  "cursor.aipreview.enabled": true,
  "cursor.chat.ghostTextAtSymbols": true,
  "cursor.chat.showSuggestedFiles": true,
  "cursor.chat.smoothStreaming": true,
  "cursor.composer.shouldAllowCustomModes": true,
  "cursor.composer.shouldChimeAfterChatFinishes": true,
  "cursor.cpp.disabledLanguages": [
    "plaintext",
    "scminput"
  ],
  "cursor.cpp.enablePartialAccepts": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.accessibilitySupport": "on",
  "editor.defaultFormatter": "biomejs.biome",
  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": true,
  "editor.formatOnSave": true,
  "editor.inlineSuggest.enabled": true,
  "editor.linkedEditing": true,
  "editor.semanticTokenColorCustomizations": {},
  "editor.tabSize": 2,
  "explorer.compactFolders": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmPasteNative": false,
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb, next.config.mjs, postcss.config.js, tailwind.config.ts, tsconfig.json, sst-env.d.ts, .gitignore, .eslintrc.json, components.json, .prettierrc, eslint.config.mjs"
  },
  "files.exclude": {
    "**/.git": false
  },
  "git.autofetch": true,
  "git.confirmSync": false,
  "github.copilot.enable": {
    "*": false,
    "markdown": true,
    "plaintext": false,
    "scminput": false,
    "yaml": true
  },
  "javascript.inlayHints.enumMemberValues.enabled": true,
  "javascript.inlayHints.functionLikeReturnTypes.enabled": true,
  "javascript.inlayHints.parameterNames.enabled": "all",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "kilo-code.allowedCommands": [
    "npm test",
    "npm install",
    "tsc",
    "git log",
    "git diff",
    "git show"
  ],
  "material-icon-theme.folders.theme": "specific",
  "redhat.telemetry.enabled": false,
  "snyk.advanced.cliPath": "/Users/clopca/Library/Application Support/snyk-ls/snyk-macos",
  "snyk.yesWelcomeNotification": false,
  "sonarlint.rules": {
    "java:S1220": {
      "level": "off"
    },
    "java:S6242": {
      "level": "off"
    },
    "java:S6244": {
      "level": "off"
    },
    "typescript:S1848": {
      "level": "off"
    },
    "typescript:S2004": {
      "level": "off"
    },
    "typescript:S6479": {
      "level": "on"
    },
    "typescript:S6772": {
      "level": "off"
    }
  },
  "tailwind-fold.autoFold": false,
  "terminal.integrated.automationProfile.osx": {},
  "terminal.integrated.defaultProfile.osx": "zsh",
  "terminal.integrated.env.osx": {
    "Q_NEW_SESSION": "1"
  },
  "terminal.integrated.fontFamily": "'MesloLGS NF'",
  "terminal.integrated.fontLigatures": true,
  "typescript.inlayHints.parameterNames.enabled": "all",
  "typescript.inlayHints.parameterTypes.enabled": true,
  "typescript.inlayHints.propertyDeclarationTypes.enabled": true,
  "typescript.inlayHints.variableTypes.enabled": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "vs-kubernetes": {
    "vscode-kubernetes.helm-path.mac": "/Users/clopca/.vs-kubernetes/tools/helm/darwin-arm64/helm",
    "vscode-kubernetes.minikube-path.mac": "/Users/clopca/.vs-kubernetes/tools/minikube/darwin-arm64/minikube"
  },
  "window.zoomLevel": 1,
  "workbench.activityBar.location": "top",
  "workbench.activityBar.orientation": "vertical",
  "workbench.colorTheme": "Dracula Pro (Van Helsing)",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.settings.applyToAllProfiles": [
    "editor.formatOnSave"
  ],
  "yaml.customTags": [
    "!And",
    "!And sequence",
    "!If",
    "!If sequence",
    "!Not",
    "!Not sequence",
    "!Equals",
    "!Equals sequence",
    "!Or",
    "!Or sequence",
    "!FindInMap",
    "!FindInMap sequence",
    "!Base64",
    "!Join",
    "!Join sequence",
    "!Cidr",
    "!Ref",
    "!Sub",
    "!Sub sequence",
    "!GetAtt",
    "!GetAZs",
    "!ImportValue",
    "!ImportValue sequence",
    "!Select",
    "!Select sequence",
    "!Split",
    "!Split sequence"
  ],
  "amp.url": "https://ampcode.com/",
  "editor.wordBasedSuggestions": "off",
  "amp.tab.enabled": true,
  "amp.mcpServers": {
    "playwright": {
      "command": "npx",
      "args": [
        "-y",
        "@playwright/mcp@latest",
        "--headless",
        "--isolated"
      ]
    }
  }
}